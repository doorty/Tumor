body.subtumor #dialog {
  position: absolute;
  top: 100px;
  left: 100px;
  width: 350px; }
  body.subtumor #dialog ul {
    list-style-type: none;
    display: block;
    margin: 0;
    padding: 0; }
    body.subtumor #dialog ul li {
      float: right; }
      body.subtumor #dialog ul li a {
        display: inline-block;
        padding: 9px 25px;
        margin: 0 0 0 1px;
        text-decoration: none;
        background-color: gray;
        -webkit-border-radius: 10px 10px 0 0;
        -moz-border-radius: 10px 10px 0 0;
        -o-border-radius: 10px 10px 0 0;
        -ms-border-radius: 10px 10px 0 0;
        border-radius: 10px 10px 0 0;
        color: white; }
        body.subtumor #dialog ul li a.selected {
          background-color: #fcfcfc;
          color: black; }
  body.subtumor #dialog #dialog-wrapper {
    clear: both;
    padding: 15px;
    background-color: #fcfcfc;
    -webkit-border-radius: 10px 0 10px 10px;
    -moz-border-radius: 10px 0 10px 10px;
    -o-border-radius: 10px 0 10px 10px;
    -ms-border-radius: 10px 0 10px 10px;
    border-radius: 10px 0 10px 10px; }
  body.subtumor #dialog #dialog-content, body.subtumor #dialog #dialog-citations {
    height: 150px;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    padding: 5px; }
    body.subtumor #dialog #dialog-content ul li, body.subtumor #dialog #dialog-citations ul li {
      padding-bottom: 10px; }
